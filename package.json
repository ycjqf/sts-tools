{
  "name": "sts-tools",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently 'npm:watch:*'",
    "build": "graphql-codegen && npx tailwindcss -i tailwind.input.css -o ./dist/output.css && tsc && vite build",
    "preview": "vite preview --port 8798 --host",
    "lint": "npx eslint src/**/*.{ts,tsx}",
    "prettier": "npx prettier --write .",
    "generate": "graphql-codegen",
    "watch:vite": "vite",
    "watch:tailwind": "npx tailwindcss -i tailwind.input.css -o ./dist/output.css --watch",
    "watch:generate": "npm run generate && onchange 'graphql/documents/**/*.graphql' -- npm run generate"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-urql": "^3.5.12",
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "graphql": "^16.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "recharts": "^2.1.11",
    "urql": "^2.2.2",
    "zustand": "^4.0.0-rc.3"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
